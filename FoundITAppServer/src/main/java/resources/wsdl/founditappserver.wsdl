  <wsdl:message name="FoundITAppRequestMsg">
    <wsdl:part element="msgdef:inputMessage" name="inputreq"/>
  </wsdl:message>
  <wsdl:message name="FoundITAppResponseMsg">
    <wsdl:part element="msgdef:inputMessage" name="inputresp"/>
  </wsdl:message>
	
  <wsdl:portType name="foundITAppPT">
    <wsdl:operation name="check">
      <wsdl:input message="tns:FoundITAppRequestMsg"/>
      <wsdl:output message="tns:FoundITAppResponseMsg"/>
    </wsdl:operation>
  </wsdl:portType>
  
  <wsdl:binding name="FoundITAppSOAPBinding" type="tns:foundITAppPT">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="check">
      <soap:operation soapAction="http://www.example.org/founditappserver/check"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
	
  <wsdl:service name="FoundITAppServer">
    <wsdl:port binding="tns:FoundITAppSOAPBinding" name="FoundITAppServerSOAP">
      <soap:address location="http://www.example.org/"/>
    </wsdl:port>
  </wsdl:service>
