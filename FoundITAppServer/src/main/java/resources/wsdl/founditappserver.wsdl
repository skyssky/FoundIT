		<xsd:schema xmlns="http://www.w3.org/2001/XMLSchema">
			<xsd:import namespace="http://soacourse.unsw.edu.au/founditappdefinitions"
				schemaLocation="../xsd/founditappmessages.xsd" />
		</xsd:schema>
	</wsdl:types>
	
